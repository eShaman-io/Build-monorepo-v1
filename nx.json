{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist/**",
        "{projectRoot}/.next/**"
      ],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "test": {
      "outputs": [
        "{projectRoot}/coverage/**"
      ],
      "cache": true
    },
    "dev": {
      "cache": false
    }
  }
}